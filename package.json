		"eslint-plugin-boundaries": "^4.2.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-storybook": "^0.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"prettier": "^2.8.8",
		"sass": "^1.77.6",
		"storybook": "^8.1.10",
		"stylelint": "^16.3.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-standard-scss": "^13.0.0",
		"stylelint-scss": "^6.3.2",
		"typescript": "5"
	}
}
