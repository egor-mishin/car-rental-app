{
	"name": "car-rental-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"pre-commit": "lint-staged",
		"test": "jest-test",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
		"*.{ts,tsx}": [
			"prettier --write --ignore-unknown",
			"next lint --file"
		],
		"*.{css,scss,md}": [
			"prettier --write",
			"stylelint --fix"
		]
	},
	"dependencies": {
		"next": "14.2.4",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@types/lint-staged": "^13.3.0",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "14.2.4",
		"eslint-config-prettier": "^9.1.0",
		"stylelint": "^16.3.1",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-config-standard-scss": "^13.0.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"prettier": "^2.8.8",
		"sass": "^1.77.6",
		"typescript": "5"
	}
}
