{
	"name": "car-rental-app",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"pre-commit": "lint-staged",
		"test": "jest-test",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown",
